@namespace("com.mkwhitacre.conway.kafka.stream")
protocol CellProtocol {

     record Coord{

        long x;

        long y;

     }

     /**
     * Record that represents a person and the friends they have
     */
     record Cell{
     
         Coord coordinates;

         /**
         * The generation this cell exists in
         */
         long generation = -1;

         boolean alive = false;

         int neighborCount = 0;

     }
}
